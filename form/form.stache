<can-import from="./form.css!"/>

<div class="cs-form {{elementClass}} {{visibilityClass}}">
    <form>
        {{#if title}}
        <div class="cs-form-heading">
            <h2 class="cs-form-title">{{title}}</h2>
        </div>
        {{/if}}
        <div class="cs-form-body">
            <content></content>
        </div>
    </form>
</div>
