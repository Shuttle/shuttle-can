{{collapsed}}
<div class="card">
    <div class="card-header {{elementClass}}" id="collapse-{{collapseId}}-header" data-toggle="collapse"
         data-target="#{{collapseId}}" aria-expanded="true"
         aria-controls="{{collapseId}}">
        <h5 class="mb-0">
            {{text}}
        </h5>
    </div>

    <div id="{{collapseId}}" class="collapse show"
         aria-labelledby="{{collapseId}}-header" {{#if(accordionId)}}data-parent="#{{accordionId}}"{{/if}}>
        <div class="card-body">
            <content />
        </div>
    </div>
</div>