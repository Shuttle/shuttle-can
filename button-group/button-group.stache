<div class="btn-group" role="group">
    {{#each buttons}}
        <button type:from="type" class="btn {{elementClass}} {{visibilityClass}}" on:click="_click(scope.event)"
                disabled:bind="disabled">
            {{#if iconNameClass}}<i class="{{iconClass}} {{iconNameClass}}{{#if text}} {{iconSpacingClass}}{{/if}}" aria-hidden="true"></i>{{/if}}{{text}}
        </button>
    {{/each}}
</div>